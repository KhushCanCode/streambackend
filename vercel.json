{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*).mp4",
        "headers": {
          "Content-Type": "video/mp4"
        },
        "dest": "/public/$1.mp4"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  